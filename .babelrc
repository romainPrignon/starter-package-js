{
  "presets": [
    [
      "env",
      {
        "targets": {
          "browsers": ["last 2 versions"],
          "node": 4
        }
      }
    ]
  ],
  "plugins": [
    "transform-flow-strip-types",
    "transform-object-rest-spread",
    ["module-resolver", {
      "root": ["./src"] }
    ]
  ]
}
