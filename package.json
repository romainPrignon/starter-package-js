{
  "name": "starter-package-js",
  "version": "1.0.0",
  "description": "module description",
  "author": "Romain Prignon <pro.rprignon@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/romainPrignon/starter-package-js.git"
  },
  "main": "dist/main.js",
  "engines": {
    "node": ">=4.x",
    "npm": ">= 2.x"
  },
  "scripts": {
    "build": "npm run clean && babel main.js -d dist",
    "clean:all": "rimraf -- dist coverage node_modules",
    "clean": "rimraf -- dist",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0",
    "changelog:lint": "conventional-changelog-lint -f || true",
    "coverage": "istanbul cover jasmine",
    "inspect": "node-inspector",
    "lint": "eslint src/ spec/",
    "precommit": "npm run lint && npm run test",
    "release": "release-it",
    "test": "babel-node spec/support/run.js",
    "test:debug": "node debug ./node_modules/.bin/jasmine",
    "test:watch": "chokidar '**/*.js' -c 'npm run test'"
  },
  "dependencies": {
    "config": "latest"
  },
  "devDependencies": {
    "babel-cli": "latest",
    "babel-preset-es2015": "latest",
    "babel-preset-es2017": "latest",
    "babel-register": "latest",
    "chokidar-cli": "latest",
    "conventional-changelog-cli": "latest",
    "conventional-changelog-lint": "latest",
    "eslint": "latest",
    "husky": "latest",
    "istanbul": "latest",
    "jasmine": "latest",
    "node-inspector": "latest",
    "release-it": "latest",
    "rimraf": "latest"
  }
}
